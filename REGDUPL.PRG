CLEAR
SET TALK OFF
SELECT NNF, VEN FROM CADNF GROUP BY NNF,VEN;
   ORDER BY NNF;
   INTO TABLE WORK
CLOSE DATA
SELE 0
USE CABECNF
INDEX ON NNF TO TST
SELE 0
USE WORK
GO TOP
*RETURN
DO WHILE ! EOF() 
  IF SEEK( NNF, 'CABECNF' ) .AND. WORK->VEN <> CABECNF->VEN
    SELE CABECNF
    DELETE
  ENDIF
  SELECT WORK  
  SKIP
ENDDO  
SET TALK ON
CLOSE DATA