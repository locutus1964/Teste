set date to british
CLOSE DATA
SELE 0
USE DUPL INDEX DUPL
REPL ALL DTPGCOM WITH {}  
REPL ALL PGCOMIS WITH .T.
SELE DUPL
go top
DO WHILE ! EOF()
  IF ! EMPTY(CTOD(PAG))
    DT = CTOD( '15/' + SUBSTR(PAG,4,5))
  ELSE
    DT = CTOD( '15/' + SUBSTR(VENC,4,5))
  ENDIF  
  REPL DTPGCOM WITH  DT
  REPL PGCOMIS WITH .T. 
  SKIP
ENDDO  

