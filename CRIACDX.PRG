PROCEDURE DUPLCDX
*****************

  USE DUPL
  DELETE TAG ALL
  PACK
  INDEX ON STR(NDUP,6) + STR(PDUP,1) + STR(YEAR(EMIS),4) TAG XDUPANO
  INDEX ON CGC + STR(NDUP,6) + STR(PDUP,1) + DTOS(EMIS)  TAG XDUPCGC
  INDEX ON STR(NDUP,6) + STR(PDUP,1)                     TAG P_DUP
  INDEX ON CGC 		                                     TAG XCGC

RETURN  

