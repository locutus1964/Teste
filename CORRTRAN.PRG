USE CLIEN
DO WHILE .NOT. EOF()
  IF ! EMPTY(TRA) .AND. SUBSTR(TRA, 4, 1) = ' ' THEN
    REPLACE TRA     WITH '0' + TRA
  ENDIF
  IF ! EMPTY(TRA_OPC) .AND. SUBSTR(TRA_OPC, 4, 1) = ' ' THEN  
    REPLACE TRA_OPC WITH '0' + TRA_OPC
  ENDIF  
  
  SKIP
ENDDO
