dINICIO = {01/01/96}
dFIM   = {25/07/97}
nRegiao = '2200'
nVen = '2201'


SELECT CLI.GUE, CLI.EST, PED.PINT, PED.PED, PED.DTPEDREP, PED.DEM,;
       (PED.VALPED + PED.VALIPI), CABNF.NNF, CABNF.DEMISSAO, (CABNF.VALPROD + CABNF.VALIPI),;
       NF.TRA, TRA.RAZ, NF.ENT;
  FROM CLIEN CLI,;
       CABECNF CABNF,;
       CADNF NF,;
       PEDIC PED,;
       TRANS TRA;
  GROUP BY NF.NNF;
  WHERE (CABNF.CGC=CLI.CGC) AND;
        (CABNF.NNF=NF.NNF) AND;
        (PED.PED='NF.PED) AND;
        (NF.TRA=TRA.TRA);
  INTO TABLE TST
CLOSE ALL



