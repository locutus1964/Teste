CLEAR
CLOSE ALL
SELECT;
  IT.REF, P.DES, P.TIPO, P.CONTEUDO, SUM(IT.QTD) AS QTD, TAB.PRECO;
  FROM;
    "CABECNF.DBF" NF, "TPNF.DBF" TP,; 
    "CADNF.DBF" IT,   "PROD.DBF" P,  "TABELA.DBF" TAB;
  WHERE;
    (NF.DEMISSAO BETWEEN {01/03/96} AND {28/02/99});
    AND (NF.CANCELADA = .F. AND (NF.TIPNF = TP.TIPO AND TP.VENDA = "S"));
    AND (NF.NNF = IT.NNF) ;
    AND (IT.REF = P.REF)  ; 
    AND (P.REF = TAB.REF) ; 
  GROUP BY;
    IT.REF, P.CONTEUDO, TAB.PRECO;  
  INTO TABLE V_AV
CLOSE ALL      



