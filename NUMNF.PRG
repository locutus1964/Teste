CLEAR
SET TALK OFF
*SET STATUS OFF
CLOSE DATA
SELECT;
   PED.PED, ITP.ITE, ITP.REF, ITP.NNF;
 FROM;
   PEDIC PED, PEDID ITP;
 WHERE;
   PED.PED = ITP.PED;  
 ORDER BY;
   PED.PED, ITP.ITE;
 INTO;
   TABLE NF
   
CLOSE DATA  
=Atual()
SET TALK ON
SET STATUS ON
RETURN 

***************
PROCEDURE ATUAL
***************
  SELE 0
  USE CADNF ALIAS ITNF INDEX TMP
  REPLACE ALL NITEM WITH 0
  SELE 0
  USE NF 
  GO TOP
  DO WHILE ! EOF()
    SELECT ITNF
    IF SEEK( STR(NF->NNF,6)+NF->REF )
      REPLACE ITNF->NITEM WITH NF->ITE
    ENDIF
    select nf
    SKIP
  ENDDO  
RETURN  
